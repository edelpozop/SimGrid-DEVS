<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<atomicType modelName="trafficlights" host="localhost" xmlns="http://www.saurabh-mittal.com/NewXMLSchema">
    <states>
        <state>Manual</state>
        <state>ToAutomatic</state>
        <state>GreenLight</state>
        <state>YellowLight</state>
        <state>RedLight</state>
        <state>ToManual</state>
    </states>
    <TimeAdvance>
        <ta>
            <state>Manual</state>
            <Timeout>15.0</Timeout>
        </ta>
        <ta>
            <state>ToAutomatic</state>
            <Timeout>0.0</Timeout>
        </ta>
        <ta>
            <state>GreenLight</state>
            <Timeout>57.0</Timeout>
        </ta>
        <ta>
            <state>YellowLight</state>
            <Timeout>3.0</Timeout>
        </ta>
        <ta>
            <state>RedLight</state>
            <Timeout>60.0</Timeout>
        </ta>
        <ta>
            <state>ToManual</state>
            <Timeout>0.0</Timeout>
        </ta>
    </TimeAdvance>
    <LamdaSet>
        <lamda>
            <state>ToAutomatic</state>
            <outport>outshow_red</outport>
        </lamda>
        <lamda>
            <state>Manual</state>
            <outport>outOn</outport>
        </lamda>
        <lamda>
            <state>ToManual</state>
            <outport>outOff</outport>
        </lamda>
        <lamda>
            <state>YellowLight</state>
            <outport>outshow_red</outport>
        </lamda>
        <lamda>
            <state>GreenLight</state>
            <outport>outshow_yellow</outport>
        </lamda>
        <lamda>
            <state>RedLight</state>
            <outport>outshow_green</outport>
        </lamda>
        <lamda>
            <state>ToManual</state>
            <outport>outManual</outport>
        </lamda>
    </LamdaSet>
    <inports>
        <inport>intoManual</inport>
    </inports>
    <outports>
        <outport>outshow_red</outport>
        <outport>outOn</outport>
        <outport>outOff</outport>
        <outport>outshow_red</outport>
        <outport>outshow_yellow</outport>
        <outport>outshow_green</outport>
        <outport>outManual</outport>
    </outports>
    <deltint>
        <InternalTransition intTransitionID="6">
            <transition>
                <StartState>ToAutomatic</StartState>
                <NextState>RedLight</NextState>
            </transition>
        </InternalTransition>
        <InternalTransition intTransitionID="5">
            <transition>
                <StartState>Manual</StartState>
                <NextState>ToAutomatic</NextState>
            </transition>
        </InternalTransition>
        <InternalTransition intTransitionID="4">
            <transition>
                <StartState>ToManual</StartState>
                <NextState>Manual</NextState>
            </transition>
        </InternalTransition>
        <InternalTransition intTransitionID="3">
            <transition>
                <StartState>YellowLight</StartState>
                <NextState>RedLight</NextState>
            </transition>
        </InternalTransition>
        <InternalTransition intTransitionID="2">
            <transition>
                <StartState>GreenLight</StartState>
                <NextState>YellowLight</NextState>
            </transition>
        </InternalTransition>
        <InternalTransition intTransitionID="1">
            <transition>
                <StartState>RedLight</StartState>
                <NextState>GreenLight</NextState>
            </transition>
        </InternalTransition>
    </deltint>
    <deltext>
        <ExternalTransition extTransitionID="3">
            <IncomingMessage>toManual</IncomingMessage>
            <transition>
                <StartState>GreenLight</StartState>
                <NextState>ToManual</NextState>
            </transition>
            <scheduleIndicator>true</scheduleIndicator>
        </ExternalTransition>
        <ExternalTransition extTransitionID="2">
            <IncomingMessage>toManual</IncomingMessage>
            <transition>
                <StartState>YellowLight</StartState>
                <NextState>ToManual</NextState>
            </transition>
            <scheduleIndicator>true</scheduleIndicator>
        </ExternalTransition>
        <ExternalTransition extTransitionID="1">
            <IncomingMessage>toManual</IncomingMessage>
            <transition>
                <StartState>RedLight</StartState>
                <NextState>ToManual</NextState>
            </transition>
            <scheduleIndicator>true</scheduleIndicator>
        </ExternalTransition>
    </deltext>
</atomicType>
